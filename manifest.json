{
	"name": "More Search Option",
	"version": "1.0.0",
	"description": "Adds a multiple search options in the right-click",
	"manifest_version": 3,
	"author": "SlimyRedstone",
	"permissions": ["tabs", "activeTab", "contextMenus","storage"],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"icons": {
		"16": "icons/16x16.png",
		"32": "icons/32x32.png",
		"48": "icons/48x48.png",
		"128": "icons/128x128.png"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"host_permissions": ["http://*/*", "https://*/*"]
}
